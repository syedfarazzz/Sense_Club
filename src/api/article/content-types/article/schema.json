{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "post_title": {
      "type": "string",
      "required": true
    },
    "question": {
      "type": "boolean",
      "required": true
    },
    "upload_pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "tags": {
      "type": "json",
      "required": true
    },
    "social_link": {
      "type": "text"
    },
    "likes": {
      "type": "json"
    },
    "comments": {
      "type": "json"
    },
    "categories": {
      "type": "enumeration",
      "enum": [
        "General",
        "Health",
        "Product",
        "Routine",
        "Services",
        "Limited Time Offer"
      ],
      "required": true
    },
    "publisher_id": {
      "type": "text"
    },
    "publisher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "articles"
    }
  }
}
